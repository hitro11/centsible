{
  "include": [
    "src/**/*", "./environment.d.ts"
  ],
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
  }
}